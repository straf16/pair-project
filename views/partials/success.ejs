<%if(success){ %>
    <div id="succes">
        <div class="alert alert-success" role="alert">
            <%= success %>
            <button onclick="this.parentElement.style.display='none'" type="button" class="close" data-dismiss="alert" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
    </div>
<% } %>